{
  "expo": {
    "name": "Qanun Qapısı",
    "slug": "qanun-qapisi",
    "version": "2.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/qanun.png",
    "scheme": "qanunqapisimobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.creadev.qanunqapisi",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "vmi2809419.contaboserver.net": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionRequiresForwardSecrecy": true,
              "NSExceptionMinimumTLSVersion": "TLSv1.2",
              "NSIncludesSubdomains": true,
              "NSRequiresCertificateTransparency": false
            }
          }
        }
      }
    },
    "android": {
      "package": "com.qanunqapisi.mobile",
      "adaptiveIcon": {
        "backgroundColor": "#7313e8",
        "foregroundImage": "./assets/images/qanun.png"
      },
      "usesCleartextTraffic": false,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/qanun.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#7313e8",
          "dark": {
            "backgroundColor": "#7313e8"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3f54983d-9286-4e78-887c-5c37c823546a"
      }
    },
    "owner": "fakhri2406"
  }
}
